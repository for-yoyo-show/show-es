{
  "name": "room-es3",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env ENV_LWD=development webpack serve  --mode=development --config script/webpack.config.js",
    "build": "cross-env ENV_LWD=production webpack --mode=production --config script/webpack.config.js",
    "lint:tsx": "eslint --ext .tsx src && eslint --ext .ts src",
    "lint:css": "stylelint --aei .less .css src",
    "precommit": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "precommit",
    "precommit-msg"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.2",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-less": "^1.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.4.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "three": "^0.152.0"
  }
}
